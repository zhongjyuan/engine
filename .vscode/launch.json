	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "server|dev",
			"skipFiles": ["<node_internals>/**"],
			"program": "${workspaceFolder}\\server.js"
		},
		{
			"type": "node",
			"request": "launch",
			"name": "server|pro",
			"skipFiles": ["<node_internals>/**"],
			"program": "${workspaceFolder}\\dist\\server.js"
		},
		{
			"type": "node",
			"request": "launch",
			"name": "wechatbot|dev",
			"skipFiles": ["<node_internals>/**"],
			"program": "${workspaceFolder}\\modules\\wechatbot\\main.js"
		},
		{
			"type": "node",
			"request": "launch",
			"name": "wechatbot|pro",
			"skipFiles": ["<node_internals>/**"],
			"program": "${workspaceFolder}\\dist\\wechatbot.js"
		},
		{
			"type": "go",
			"mode": "auto",
			"request": "launch",
			"name": "wechatgpt",
			"program": "${workspaceFolder}\\modules\\wechatgpt\\main.go",
			"showLog": true,
			// "env": {
			// 	"GOPATH": "D:/Program Files/Go_Works",
			// 	"GOROOT": "D:/Program Files/Go"
			// },
			"args": []
		}
	]
